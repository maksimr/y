{
  "name": "playground",
  "version": "0.0.0",
  "main": "./src/index.js",
  "scripts": {
    "start": "webpack-dev-server -d",
    "build": "webpack -p",
    "test": "jest --passWithNoTests",
    "lint": "eslint src/"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "immer": "7.0.9",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/eslint-parser": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "babel-loader": "8.1.0",
    "css-loader": "4.3.0",
    "eslint": "7.9.0",
    "html-webpack-plugin": "4.4.1",
    "jest": "26.4.2",
    "jest-cli": "26.4.2",
    "style-loader": "1.2.1",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "eslintConfig": {
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "requireConfigFile": false,
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "no-undef": "warn",
      "no-unused-vars": "warn",
      "valid-jsdoc": [
        "warn",
        {
          "requireReturn": false,
          "requireParamDescription": false,
          "requireReturnDescription": false
        }
      ]
    }
  }
}
